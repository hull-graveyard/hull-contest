    <title>Contest</title>

    <%= stylesheet_link_tag 'https://hullstrap.s3.amazonaws.com/stylesheets/hullstrap.css' %>
    <%= stylesheet_link_tag 'all' %>

    <%= javascript_include_tag '//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js' %>
